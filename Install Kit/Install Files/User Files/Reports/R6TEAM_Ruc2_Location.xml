<CustomReport  title="R6TEAM_Rucksack2_Locations" menupath="System Supplied\Rucksack " 

description="Reports on the Locationsin a selected Rucksack" >



<SQL>

<Where keytype="Default">


Select Location_Key,Item_Name as Location_Name FROM 
LOCATION_NAME WHERE Preferred = 1 AND
<Condition field= "LOCATION_NAME.LOCATION_KEY" entrycount ="-1" operator= "equal" type ="Location"  name= "SelectRucksack" userucksack= "yes" />



   
   

                  
</Where>

</SQL>
